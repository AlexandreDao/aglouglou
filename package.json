{
  "name": "aglouglou",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "expo prebuild --clean && node scripts/update-gradle.js",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build-android": "eas build --platform android --profile development",
    "build-ios": "eas build --platform ios --profile development-simulator",
    "test-debug": "jest --watchAll",
    "test": "jest --coverage=false",
    "test-coverage": "jest",
    "test-update": "jest --updateSnapshot --coverage=false",
    "lint": "eslint .",
    "e2e-android": "maestro --platform android test e2e",
    "e2e-ios": "maestro --platform ios test e2e",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/.eslintrc.js.js",
      "!**/.eslintrc.js.js",
      "!**/.expo/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ]
  },
  "dependencies": {
    "@dev-plugins/react-query": "~0.4.0",
    "@expo/vector-icons": "~15.0.3",
    "@gorhom/bottom-sheet": "~5.2.8",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "~2.0.2",
    "@tanstack/react-query": "~5.90.12",
    "@tanstack/react-query-persist-client": "~5.90.14",
    "axios": "~1.13.2",
    "date-fns": "~4.1.0",
    "expo": "~54.0.29",
    "expo-blur": "~15.0.8",
    "expo-checkbox": "~5.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "^8.0.10",
    "expo-router": "~6.0.19",
    "expo-speech-recognition": "~1.1.0",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "immer": "~11.0.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "~1.18.5",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "zustand": "~5.0.9"
  },
  "devDependencies": {
    "@react-native-community/cli": "latest",
    "@react-native/eslint-config": "0.81.5",
    "@testing-library/react-native": "~13.3.3",
    "@types/jest": "~29.5.13",
    "@types/react": "~19.1.17",
    "@typescript-eslint/parser": "~8.50.0",
    "eslint": "~9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jest": "~29.5.0",
    "eslint-plugin-prettier": "~5.2.1",
    "husky": "~8.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "lint-staged": "~16.2.7",
    "prettier": "~3.7.4",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}
