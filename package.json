{
  "name": "aglouglou",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "expo prebuild --clean",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test-debug": "jest --watchAll",
    "test": "jest --coverage=false",
    "test-coverage": "jest",
    "test-update": "jest --updateSnapshot --coverage=false",
    "lint": "expo lint",
    "lint-fix": "eslint . --fix",
    "e2e": "maestro test e2e",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/.eslintrc.js.js",
      "!**/.eslintrc.js.js",
      "!**/.expo/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|@react-navigation|react-native|react-native-reanimated|@react-native|react-native-safe-area-context|expo|@expo|expo-modules-core|expo-image|expo-asset|expo-constants|expo-blur|expo-font|expo-linking|expo-router|expo-splash-screen|expo-status-bar|expo-symbols|expo-system-ui|expo-web-browser|expo-haptics)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ]
  },
  "dependencies": {
    "@dev-plugins/react-query": "~0.1.0",
    "@expo/vector-icons": "~14.0.2",
    "@gorhom/bottom-sheet": "~5.0.6",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/flash-list": "~1.7.1",
    "@tanstack/react-query": "~5.62.9",
    "@testing-library/react-native": "^12.9.0",
    "axios": "~1.7.9",
    "date-fns": "~4.1.0",
    "expo": "~52.0.28",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.5",
    "expo-dev-client": "~5.0.10",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-speech-recognition": "^1.1.0",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.1",
    "expo-system-ui": "~4.0.7",
    "expo-web-browser": "~14.0.2",
    "immer": "^10.1.1",
    "react": "~18.3.1",
    "react-native": "~0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-controller": "^1.16.0",
    "react-native-mmkv": "~3.2.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-webview": "~13.12.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@react-native-community/cli": "latest",
    "@react-native/eslint-config": "^0.76.6",
    "@tanstack/eslint-plugin-query": "~5.62.9",
    "@testing-library/jest-native": "~5.4.3",
    "@types/jest": "~29.5.12",
    "@types/react": "~18.3.12",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "~8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "~5.2.1",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.3",
    "lint-staged": "^15.3.0",
    "prettier": "~3.4.2",
    "typescript": "~5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}
